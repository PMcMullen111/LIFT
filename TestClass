		if (event.getSource() == setNextWeek && weekBox.getSelectedIndex()==0) {

			for( int v=0;  v<20; v++){
			weekTwoArrayEx[v]=weekOneArrayEx[v];
			weekTwoArraySet[v]=weekOneArraySet[v];
			
			System.out.println(weekTwoArrayEx[v]);
			System.out.println(weekTwoArraySet[v]);
			
			if(isNumeric(weekOneArrayPercent[v])==true){
